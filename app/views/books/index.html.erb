<p id="notice"></p>
<h1>Books</h1>

<% @books.each do |book| %>

  <p>タイトル</p>
  <span><%= book.title %></span>

<% end %>

<h2>New book</h2>
<!-- form_with部分 -->
<%= form_with model: @book do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <%= f.submit '投稿' %>
<% end %>